ROOTDIR=..
!include "$(ROOTDIR)\system.mak"

all:
	cd $(MAKEDIR)\basicproxy"
	@$(MAKE) /NOLOGO /$(MAKEFLAGS)
	cd $(MAKEDIR)

clean:
	cd $(MAKEDIR)\basicproxy"
	@$(MAKE) /NOLOGO /$(MAKEFLAGS) clean
	cd $(MAKEDIR)

test:
	cd $(MAKEDIR)\basicproxy"
	@$(MAKE) /NOLOGO /$(MAKEFLAGS) test
	cd $(MAKEDIR)


